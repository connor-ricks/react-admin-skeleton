{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@authentication/*": ["app/authentication/*"],
      "@components/*": ["app/components/*"],
      "@contexts/*": ["app/contexts/*"],
      "@com/*": ["app/com/*"]
    },
    "module": "esnext",
    "moduleResolution": "node",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": ["**/*.js", "**/*.jsx"],
  "exclude": ["node_modules"]
}
